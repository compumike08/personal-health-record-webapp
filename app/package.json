    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  }
